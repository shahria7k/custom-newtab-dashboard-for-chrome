{
    "manifest_version": 3,
    "name": "Custom New Tab",
    "description": "A custom new tab page for Chrome.",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage",
        "system.cpu",
        "system.memory",
        "system.storage",
        "system.display",
        "system.storage",
        "tabs",
        "scripting",
        "webNavigation",
        "webRequest"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}